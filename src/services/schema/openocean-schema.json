{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "apiKey": { "type": "string", "nullable": true },
    "allowedSlippage": { "type": "string" },
    "gasLimitEstimate": { "type": "integer" },
    "ttl": { "type": "integer" },
    "contractAddresses": {
      "type": "object",
      "patternProperties": {
        "[\\w-]+$": {
          "type": "object",
          "patternProperties": {
            "^\\w+$": {
              "type": "object",
              "properties": {
                "routerAddress": { "type": "string" }
              },
              "required": ["routerAddress"],
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "enabledDexCodes": {
      "type": "object",
      "patternProperties": {
        "[\\w-]+$": {
          "type": "object",
          "patternProperties": {
            "^\\w+$": {
              "type": ["array", "null"]
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": [
    "allowedSlippage",
    "gasLimitEstimate",
    "ttl",
    "contractAddresses"
  ]
}
