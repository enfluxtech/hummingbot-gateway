{
  "version": "0.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NEAR_ENV=mainnet tsdx build --format cjs,esm,umd",
    "build:testnet": "NEAR_ENV=testnet tsdx build --format cjs,esm,umd",
    "build:mainnet": "NEAR_ENV=mainnet tsdx build --format cjs,esm,umd",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "NEAR_ENV=testnet tsdx build --format cjs,esm,umd",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ref-sdk",
  "author": "Max",
  "module": "dist/ref-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/ref-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ref-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.6",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/big.js": "^6.1.5",
    "@types/bn.js": "^5.1.1",
    "@types/lodash": "^4.14.182",
    "big.js": "^6.2.1",
    "mathjs": "^9.3.0",
    "near-api-js": "0.44.2",
    "ref-sdk": "^0.1.4"
  }
}